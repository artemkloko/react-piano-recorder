    "react-piano": "^3.1.3",
    "soundfont-player": "git://github.com/jnpdx/soundfont-player.git#fix/typescript-bindings"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "schema:download": "npx apollo schema:download --endpoint=http://localhost:4000/graphql graphql-schema.json",
    "schema:types": "npx apollo codegen:generate --localSchemaFile=graphql-schema.json --target=typescript --includes=src/**/* --tagName=gql --addTypename --globalTypesFile=src/types/graphql-global-types.ts --outputFlat ./src/@types/graphql/index.ts"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
