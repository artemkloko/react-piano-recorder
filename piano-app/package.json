    "typescript": "^3.5.3"
  },
  "dependencies": {
    "apollo-boost": "^0.4.3",
    "bootstrap": "^4.3.1",
    "graphql": "^14.4.2",
    "graphql-tag": "^2.10.1",
    "react": "^16.8.6",
    "react-apollo": "^2.5.8",
    "react-bootstrap": "^1.0.0-beta.10",
    "react-dom": "^16.8.6",
