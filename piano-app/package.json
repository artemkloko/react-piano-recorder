    "@types/jest": "^24.0.16",
    "@types/react": "^16.8.23",
    "@types/react-dom": "^16.8.5",
    "apollo": "^2.16.3",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
